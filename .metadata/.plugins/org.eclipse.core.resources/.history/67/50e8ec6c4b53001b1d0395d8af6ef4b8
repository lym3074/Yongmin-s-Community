src="https://code.jquery.com/jquery-3.5.1.min.js"

$(document).ready(function() {
$('#btn').on('click',function() {
    if($('#userId').val() == "") {
        $('#userId').next('label').addClass('warning');
        setTimeout(function() {
            $('label').removeClass('warning');
        }, 1000);
    }
    else if($(pw).val() == "") {
        $(pw).next('label').addClass('warning');
        setTimeout(function() {
            $('label').removeClass('warning');
        }, 1000);
    }
});
}